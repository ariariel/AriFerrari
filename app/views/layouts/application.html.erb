<!DOCTYPE html>
<html>
<head>
  <title>AriFerrari</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link href='https://fonts.googleapis.com/css?family=Raleway:400,700' rel='stylesheet' type='text/css'>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="wrapper">
		<header>
			<%= link_to image_tag("shoe_head.png", size: "128"), {:controller=>'equalizer',:action=>'index'},{:class=>'header_logo'} %>
			<%= link_to "Ari Ferrari", {:controller=>'equalizer',:action=>'index'},{:class=>'header_title'} %>
			<nav>
				<%= link_to "Resume", {:controller=>'equalizer',:action=>'index'},{:class=>'resume_anchor'} %>
				<%= link_to "Github", "http://github.com/ariariel" %>
			</nav>
		</header>
		<%= yield %>
	</div>
</body>
</html>
